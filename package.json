	},
	"devDependencies": {
		"@babel/core": "^7.4.3",
		"@babel/preset-env": "^7.4.3",
		"@wpmudev/shared-ui": "2.3.23",
		"browser-sync": "^2.26.3",
		"eslint-config-wordpress": "^2.0.0",
		"fs": "0.0.1-security",
		"gh-pages": "^2.0.1",
		"gulp": "^3.9.1",
		"gulp-autoprefixer": "^6.0.0",
		"gulp-babel": "^8.0.0",
		"gulp-clean-css": "^3.10.0",
		"gulp-concat": "^2.6.1",
		"gulp-eslint": "^5.0.0",
		"gulp-header": "^2.0.7",
		"gulp-htmlmin": "^5.0.1",
		"gulp-rename": "^1.4.0",
		"gulp-replace": "^1.0.0",
		"gulp-sass": "^4.0.2",
		"gulp-uglify": "^3.0.1",
		"gulp-uglify-es": "^1.0.4",
		"gulp-watch": "^5.0.1",
		"pump": "^3.0.0"
	},
	"eslintConfig": {
		"extends": "wordpress",
		"parserOptions": {
			"sourceType": "module",
			"ecmaVersion": 2018
		},
		"env": {
			"es6": true
		}
	},
	"eslintIgnore": [
		"library/js/select2.full.js",
		"assets/js/public.min.js",
		"assets/js/forminator-ui.min.js",
		"assets/js/public/clipboard.js"
	],
	"scripts": {
		"start": "gulp development",
		"build:library": "gulp library:build",
		"build:showcase": "gulp public:build",
		"clean:modules": "rm -rf ./node_modules/ && rm -rf ./package-lock.json",
		"clean:ghpages": "rm -rf node_modules/gh-pages/.cache",
		"release:library": "npm run build:library && npm run clean:ghpages && gh-pages -b master -d ./build/library",
		"release:showcase": "npm run clean:modules && npm install && npm run build:showcase && npm run clean:ghpages && gh-pages -b gh-pages -d ./build/public",
		"release": "npm run release:library && npm run release:showcase"
	},
	"dependencies": {}
}
